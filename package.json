    "express": "^4.19.2",
    "openai": "^4.65.0",
    "pino": "^8.19.0",
    "qrcode-terminal": "^0.12.0",
    "stripe": "^14.21.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/qrcode-terminal": "^0.12.2",
